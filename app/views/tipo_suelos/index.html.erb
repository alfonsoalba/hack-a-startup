<p id="notice"><%= notice %></p>

<h1>Tipo Suelos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_suelos.each do |tipo_suelo| %>
      <tr>
        <td><%= tipo_suelo.nombre %></td>
        <td><%= link_to 'Show', tipo_suelo %></td>
        <td><%= link_to 'Edit', edit_tipo_suelo_path(tipo_suelo) %></td>
        <td><%= link_to 'Destroy', tipo_suelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Suelo', new_tipo_suelo_path %>
